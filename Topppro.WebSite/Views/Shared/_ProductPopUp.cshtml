@using Topppro.WebSite.Extensions
@using Framework.Common.Extensions

@model Topppro.Entities.Product

@if (!Model.GetPopUp().IsEmpty()) {
    <p>
        <a href="@Model.GetPopUp().FirstOrDefault(i => i.Contains("popup"))" class="thickbox">
            <img src="@Model.GetPopUp().FirstOrDefault(i => i.Contains("side"))" alt="@Model.Name" width="163" border="0" />
        </a>
    </p>
}
